          "200880030": {
            "type": "Text",
            "id": 200880030,
            "sectionId": 2008800,
            "senderId": null,
            "text": "Can you make General Jing Yuan the example?",
            "option": "Use General Jing Yuan as an example?",
            "nextIds": [
              200880031
            ],
            "kind": "Player"
          },
          "200880031": {
            "type": "Text",
            "id": 200880031,
            "sectionId": 2008800,
            "senderId": 1208,
            "text": "Though I have been working with him for many years, Jing Yuan keeps doing this to make me strategize for him... No, this example won't do.",
            "option": null,
            "nextIds": [
              200880032
            ],
            "kind": "NPC"
          },
          "200880032": {
            "type": "Text",
            "id": 200880032,
            "sectionId": 2008800,
            "senderId": 1208,
            "text": "He had merely stirred my motivation in the short term. In the long term... I will not fall for his tricks again.",
            "option": null,
            "nextIds": [
              200880033
            ],
            "kind": "NPC"
          },
          "200880033": {
            "type": "Text",
