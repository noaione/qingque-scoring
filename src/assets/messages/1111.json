          "nextIds": [
            200820025
          ],
          "kind": "NPC"
        },
        "200820025": {
          "type": "Text",
          "id": 200820025,
          "sectionId": 2008200,
          "senderId": 1111,
          "text": "I give up, no use overthinking it",
