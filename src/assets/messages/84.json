        "messages": {
          "121680001": {
            "type": "Text",
            "id": 121680001,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "{NICKNAME}, hello.",
            "option": null,
            "nextIds": [
              121680002
            ],
            "kind": "NPC"
          },
          "121680002": {
            "type": "Text",
            "id": 121680002,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "My investigation and research on the Xianzhou is complete.",
            "option": null,
            "nextIds": [
              121680003
            ],
            "kind": "NPC"
          },
          "121680003": {
            "type": "Text",
            "id": 121680003,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "No... Rather, I have a completely new topic now.",
            "option": null,
            "nextIds": [
              121680004
            ],
            "kind": "NPC"
          },
          "121680004": {
            "type": "Text",
            "id": 121680004,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "Thank you for your companionship along the way. You are the most sincere and reliable friend I have made on Xianzhou.",
            "option": null,
            "nextIds": [
              121680005
            ],
            "kind": "NPC"
          },
          "121680005": {
            "type": "Text",
            "id": 121680005,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "So, no matter what happens, I don't want to hide anything from you or deceive you.",
            "option": null,
            "nextIds": [
              121680006
            ],
            "kind": "NPC"
          },
          "121680006": {
            "type": "Text",
            "id": 121680006,
            "sectionId": 1216800,
            "senderId": 84,
            "text": "I want to share my happiness with you. See you at the usual place.",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          }
        },
        "mission": {
          "id": 2020203,
          "name": "Todd Riordan's Academic Research: Continued",
          "type": "Branch"
        }
      }
    ],
    [
      {
