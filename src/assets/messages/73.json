        "messages": {
          "111540000": {
            "type": "Text",
            "id": 111540000,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "Where are you, March and {NICKNAME}? Are you all right?",
            "option": null,
            "nextIds": [
              111540001
            ],
            "kind": "NPC"
          },
          "111540001": {
            "type": "Text",
            "id": 111540001,
            "sectionId": 1115400,
            "senderId": 1001,
            "text": "Bronya! We're in the Underworld right now",
            "option": null,
            "nextIds": [
              111540002
            ],
            "kind": "NPC"
          },
          "111540002": {
            "type": "Text",
            "id": 111540002,
            "sectionId": 1115400,
            "senderId": 1001,
            "text": "The IPC's people took over the Great Mine. We just kicked them out",
            "option": null,
            "nextIds": [
              111540003
            ],
            "kind": "NPC"
          },
          "111540003": {
            "type": "Text",
            "id": 111540003,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "I see, you're with Seele",
            "option": null,
            "nextIds": [
              111540004
            ],
            "kind": "NPC"
          },
          "111540004": {
            "type": "Text",
            "id": 111540004,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "How are things going? Did you get hurt?",
            "option": null,
            "nextIds": [
              111540005
            ],
            "kind": "NPC"
          },
          "111540005": {
            "type": "Text",
            "id": 111540005,
            "sectionId": 1115400,
            "senderId": 1001,
            "text": "Don't worry. We can totally handle this!",
            "option": null,
            "nextIds": [
              111540006,
              111540008
            ],
            "kind": "NPC"
          },
          "111540006": {
            "type": "Text",
            "id": 111540006,
            "sectionId": 1115400,
            "senderId": null,
            "text": "I'm sorry. We weren't able to persuade Topaz...",
            "option": "I failed to persuade Topaz.",
            "nextIds": [
              111540007
            ],
            "kind": "Player"
          },
          "111540007": {
            "type": "Text",
            "id": 111540007,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "No worries, {NICKNAME}! This wasn't your responsibility in the first place",
            "option": null,
            "nextIds": [
              111540010
            ],
            "kind": "NPC"
          },
          "111540010": {
            "type": "Text",
            "id": 111540010,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "Something just happened here, and I can't explain it clearly in messages.",
            "option": null,
            "nextIds": [
              111540011
            ],
            "kind": "NPC"
          },
          "111540011": {
            "type": "Text",
            "id": 111540011,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "Could you come back to Qlipoth Fort? I need advice from you two on this issue",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "111540008": {
            "type": "Text",
            "id": 111540008,
            "sectionId": 1115400,
            "senderId": null,
            "text": "I'm afraid the IPC won't stop there",
            "option": "The IPC won't stop there.",
            "nextIds": [
              111540009
            ],
            "kind": "Player"
          },
          "111540009": {
            "type": "Text",
            "id": 111540009,
            "sectionId": 1115400,
            "senderId": 1101,
            "text": "I see. Thanks for trying to negotiate with Topaz. This wasn't your responsibility in the first place.",
            "option": null,
            "nextIds": [
              111540010
            ],
            "kind": "NPC"
          }
        }
      }
