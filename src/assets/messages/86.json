            "nextIds": [
              111490002
            ],
            "kind": "NPC"
          },
          "111490002": {
            "type": "Sticker",
            "id": 111490002,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "[Greet]",
            "option": null,
            "nextIds": [
              111490003,
              111490005
            ],
            "kind": "NPC",
            "sticker": {
              "id": 20001,
              "path": "icon/emoji/20001.png",
              "keywords": "Hey"
            }
          },
          "111490003": {
            "type": "Text",
            "id": 111490003,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Yeah, I am. Who might you be",
            "option": "Yes, I am.",
            "nextIds": [
              111490004
            ],
            "kind": "Player"
          },
          "111490004": {
            "type": "Text",
            "id": 111490004,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "That's good",
            "option": null,
            "nextIds": [
              111490007
            ],
            "kind": "NPC"
          },
          "111490007": {
            "type": "Text",
            "id": 111490007,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Anyway, please allow me to introduce myself.",
            "option": null,
            "nextIds": [
              111490008
            ],
            "kind": "NPC"
          },
          "111490008": {
            "type": "Text",
            "id": 111490008,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "I'm Wilder from the Interastral Peace Corporation's Strategic Investment Department",
            "option": null,
            "nextIds": [
              111490009
            ],
            "kind": "NPC"
          },
          "111490009": {
            "type": "Text",
            "id": 111490009,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "I'm contacting you to recommend you a video game",
            "option": null,
            "nextIds": [
              111490010,
              111490012,
              111490015
            ],
            "kind": "NPC"
          },
          "111490010": {
            "type": "Text",
            "id": 111490010,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Game? Awesome! What game?",
            "option": "What game?",
            "nextIds": [
              111490011
            ],
            "kind": "Player"
          },
          "111490011": {
            "type": "Sticker",
            "id": 111490011,
            "sectionId": 1114900,
            "senderId": null,
            "text": "[Thumbs Up]",
            "option": null,
            "nextIds": [
              111490018
            ],
            "kind": "PlayerAuto",
            "sticker": {
              "id": 20006,
              "path": "icon/emoji/20006.png",
              "keywords": "Like"
            }
          },
          "111490018": {
            "type": "Text",
            "id": 111490018,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Now, please allow me to get back to the introduction",
            "option": null,
            "nextIds": [
              111490019
            ],
            "kind": "NPC"
          },
          "111490019": {
            "type": "Text",
            "id": 111490019,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "This game is called Aetherium Wars. It's a galactic-tier, top-class AAA cultural brand spearheaded by the IPC's Marketing Development Department in collaboration with the Interastral Network and Interastral Peace Media.",
            "option": null,
            "nextIds": [
              111490020,
              111490022
            ],
            "kind": "NPC"
          },
          "111490020": {
            "type": "Text",
            "id": 111490020,
            "sectionId": 1114900,
            "senderId": null,
            "text": "That impressive?",
            "option": "It's that good?",
            "nextIds": [
              111490021
            ],
            "kind": "Player"
          },
          "111490021": {
            "type": "Text",
            "id": 111490021,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Hahaha, it's one of our blockbusters after all.",
            "option": null,
            "nextIds": [
              111490024
            ],
            "kind": "NPC"
          },
          "111490024": {
            "type": "Text",
            "id": 111490024,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Aetherium Wars is a card-collection game, but it's nothing like the ordinary trading card games you've imagined!",
            "option": null,
            "nextIds": [
              111490025
            ],
            "kind": "NPC"
          },
          "111490025": {
            "type": "Text",
            "id": 111490025,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Here, you can use the IPC's cutting-edge Aether copying technology to record any creature you've ever seen inside an Aether Coin.",
            "option": null,
            "nextIds": [
              111490026
            ],
            "kind": "NPC"
          },
          "111490026": {
            "type": "Text",
            "id": 111490026,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "And then you can print them out and use them to compete against other players! Let your Aether Spirits fight for your glory!",
            "option": null,
            "nextIds": [
              111490027
            ],
            "kind": "NPC"
          },
          "111490027": {
            "type": "Text",
            "id": 111490027,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "How does it sound? Pretty cool, right?",
            "option": null,
            "nextIds": [
              111490028,
              111490031,
              111490034
            ],
            "kind": "NPC"
          },
          "111490028": {
            "type": "Sticker",
            "id": 111490028,
            "sectionId": 1114900,
            "senderId": null,
            "text": "[Thumbs Up]",
            "option": "Sounds interesting.",
            "nextIds": [
              111490029
            ],
            "kind": "Player",
            "sticker": {
              "id": 20006,
              "path": "icon/emoji/20006.png",
              "keywords": "Like"
            }
          },
          "111490029": {
            "type": "Text",
            "id": 111490029,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Sounds fun. Where can I play this game?",
            "option": null,
            "nextIds": [
              111490030
            ],
            "kind": "PlayerAuto"
          },
          "111490030": {
            "type": "Text",
            "id": 111490030,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Haha, awesome! I finally got a new player for my favorite game!",
            "option": null,
            "nextIds": [
              111490037
            ],
            "kind": "NPC"
          },
          "111490037": {
            "type": "Text",
            "id": 111490037,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Anyway, if you're interested, feel free to come to the Rivet Town Plaza in Jarilo-VI to try it for yourself!",
            "option": null,
            "nextIds": [
              111490038
            ],
            "kind": "NPC"
          },
          "111490038": {
            "type": "Text",
            "id": 111490038,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "We'll be holding the Interastral Tournament Festival, a great offline carnival for all Aetherium Wars fans! Please do come and enjoy it!",
            "option": null,
            "nextIds": [
              111490039,
              111490041
            ],
            "kind": "NPC"
          },
          "111490039": {
            "type": "Text",
            "id": 111490039,
            "sectionId": 1114900,
            "senderId": null,
            "text": "What a coincidence! I'm in Jarilo-VI right now.",
            "option": "Be right there.",
            "nextIds": [
              111490040
            ],
            "kind": "Player"
          },
          "111490040": {
            "type": "Text",
            "id": 111490040,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Don't run off. I'll be there soon.",
            "option": null,
            "nextIds": [
              111490043
            ],
            "kind": "PlayerAuto"
          },
          "111490043": {
            "type": "Text",
            "id": 111490043,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "No problem! I'll be here waiting for you!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "111490041": {
            "type": "Text",
            "id": 111490041,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Uhhhh",
            "option": "Maybe later.",
            "nextIds": [
              111490042
            ],
            "kind": "Player"
          },
          "111490042": {
            "type": "Text",
            "id": 111490042,
            "sectionId": 1114900,
            "senderId": null,
            "text": "I'll go over when I have time",
            "option": null,
            "nextIds": [
              111490043
            ],
            "kind": "PlayerAuto"
          },
          "111490031": {
            "type": "Sticker",
            "id": 111490031,
            "sectionId": 1114900,
            "senderId": null,
            "text": "[Confused]",
            "option": "It's kinda mid.",
