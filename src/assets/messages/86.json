          },
          "111490032": {
            "type": "Text",
            "id": 111490032,
            "sectionId": 1114900,
            "senderId": null,
            "text": "This doesn't sound as fun as Cosmic Warfare...",
            "option": null,
            "nextIds": [
              111490033
            ],
            "kind": "PlayerAuto"
          },
          "111490033": {
            "type": "Text",
            "id": 111490033,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Cosmic Warfare? Seriously!? It's nowhere near as interesting as the Aetherium Wars!",
            "option": null,
            "nextIds": [
              111490037
            ],
            "kind": "NPC"
          },
          "111490034": {
            "type": "Sticker",
            "id": 111490034,
            "sectionId": 1114900,
            "senderId": null,
            "text": "[Shocked]",
            "option": "Calm down.",
            "nextIds": [
              111490035
            ],
            "kind": "Player",
            "sticker": {
              "id": 20008,
              "path": "icon/emoji/20008.png",
              "keywords": "Shocked"
            }
          },
          "111490035": {
            "type": "Text",
            "id": 111490035,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Why are you so excited all of a sudden?",
            "option": null,
            "nextIds": [
              111490036
            ],
            "kind": "PlayerAuto"
          },
          "111490036": {
            "type": "Text",
            "id": 111490036,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Well, to be honest, I'm actually a huge fan of Aetherium Wars myself.",
            "option": null,
            "nextIds": [
              111490037
            ],
            "kind": "NPC"
          },
          "111490022": {
            "type": "Text",
            "id": 111490022,
            "sectionId": 1114900,
            "senderId": null,
            "text": "Just get to the point...",
            "option": "Get to the point...",
            "nextIds": [
              111490023
            ],
            "kind": "Player"
          },
          "111490023": {
            "type": "Text",
            "id": 111490023,
            "sectionId": 1114900,
            "senderId": 86,
            "text": "Don't worry. I'm getting there.",
            "option": null,
            "nextIds": [
              111490024
            ],
            "kind": "NPC"
          },
          "111490012": {
            "type": "Sticker",
            "id": 111490012,
            "sectionId": 1114900,
            "senderId": null,
            "text": "[Confused]",
            "option": "Not interested.",
            "nextIds": [
