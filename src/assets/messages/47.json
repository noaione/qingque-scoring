          },
          "121620102": {
            "type": "Text",
            "id": 121620102,
            "sectionId": 1216201,
            "senderId": 47,
            "text": "I got an official notification from the Commission while I was out on a job. Those three saints from The Unshackled are in big trouble",
            "option": null,
            "nextIds": [
              121620103
            ],
            "kind": "NPC"
          },
          "121620103": {
            "type": "Text",
            "id": 121620103,
            "sectionId": 1216201,
            "senderId": null,
            "text": "What happened?",
            "option": "What happened?",
            "nextIds": [
              121620104
            ],
            "kind": "Player"
          },
          "121620104": {
            "type": "Text",
            "id": 121620104,
            "sectionId": 1216201,
            "senderId": 47,
            "text": "Go to Exalting Sanctum and you'll figure it out.",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          }
        }
      }
    ]
