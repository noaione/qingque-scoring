          "122040014": {
            "type": "Text",
            "id": 122040014,
            "sectionId": 1220400,
            "senderId": null,
            "text": "It's good for unruly youngsters to go through some tough experiences to shape them up.",
            "option": "Youths need some tempering.",
            "nextIds": [
